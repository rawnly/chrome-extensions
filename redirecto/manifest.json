{
  "version": "0.0.1",
  "manifest_version": 3,
  "name": "redirecto",
  "description": "Redirects",
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://*.npmjs.com/*",
    "*://google.com/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
